VAR x = 1.5;

DEFINE foo()
BEGIN
    VAR y = x ^ 3.2;
    RETURN y % 10;
END

DEFINE bar(par1)
BEGIN
    VAR y = *par1 * x;
    VAR z = y % 3 ? foo() : bar(&y);
    RETURN z - 1;
END

DEFINE foo_bar(par1 par2)
BEGIN
    VAR x = par1 / 2;
    VAR y = par2 % 4;
    RETURN x + y;
END