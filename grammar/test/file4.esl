DEFINE foo(par1 par2 par3 par4 par5)
BEGIN
    VAR x = par1 % par2;
    VAR y = (par3 ^ par4) % par5;
    RETURN x / y;
END

DEFINE bar()
BEGIN
    VAR x = 1;
    VAR y = 2;
    VAR z;
    WHILE((x % 7) > y)
    BEGIN
        z = foo(x y (x % y) (y^x));
        x = z * x;
    END
    PRINT("END_OF_FILE");
